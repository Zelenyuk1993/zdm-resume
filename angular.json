{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "zdm-resume": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {

            "outputPath": "dist/zdm-resume",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "src/assets/js/scripts.js"
            ],
            "allowedCommonJsDependencies": [
              "web3",
              "events",
              "@walletconnect/window-getters",
              "@walletconnect/window-metadata",
              "@walletconnect/socket-transport",
              "@walletconnect/qrcode-modal",
              "is-typedarray",
              "typedarray-to-buffer",
              "cross-fetch",
              "@walletconnect/environment",
              "bn.js",
              "js-sha3",
              "query-string",
              "@coinbase/wallet-sdk",
              "mersenne-twister",
              "@metamask/jazzicon",
              "eventemitter3",
              "aes-js",
              "scrypt-js",
              "hash.js",
              "copy-to-clipboard",
              "qrcode",
              "sturdy-websocket",
              "bech32",
              "bowser",
              "joi",
              "lodash.uniqby",
              "lodash.partition",
              "prismjs",
              "codesandbox/lib/api/define",
              "flickity",
              "tallyho-detect-provider",
              "@cedelabs/providers",
              "fortmatic",
              "@portis/web3",
              "@infinitywallet/infinity-connector",
              "buffer",
              "@stablelib/ed25519"
            ],
            "serviceWorker": true,
            "ngswConfigPath": "ngsw-config.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "zdm-resume:build:production"
            },
            "development": {
              "browserTarget": "zdm-resume:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "zdm-resume:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "src/assets/js/scripts.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "b02fb57a-634a-45db-9a22-5c124098eb21"
  }
}
